@{
    Layout = "_HorizontalLayout";
    ViewData["title"] = "ثبت موفقیت آمیز";

    var trip = (Application.Services.MrShooferORS.SearchedTrip)ViewBag.trip;

    var ticket = (Ticket)ViewBag.ticket;

    Dictionary<int, string> monthName = new Dictionary<int, string>()
    {
    {1,"فروردین" },
    {2,"اردیبهشت"},
    {3, "خرداد"},
    {4, "تیر" },
    {5,"مرداد"},
    {6, "شهریور" },
    {7, "مهر" },
    {8, "آبان" },
    {9, "آذر" },
    {10, "دی" },
    {11, "بهمن" },
    {12, "اسفند" }
    };

}

@section VendorStyles {
    <link rel="stylesheet" href="~/vendor/libs/animate-css/animate.css">
    <link rel="stylesheet" href="~/vendor/libs/flatpickr/flatpickr.css" />

    <link rel="stylesheet" href="~/vendor/libs/sweetalert2/sweetalert2.css" />
    <link rel="stylesheet" href="~/vendor/libs/animate-css/animate.css" />

}
@section PageScripts {
    <script src="~/js/extended-ui-sweetalert2.js"></script>
}

@section VendorScripts {
}

    <script src="~/vendor/libs/sweetalert2/sweetalert2.js"></script>

<script>
    Swal.fire({
        title: 'سفر ثبت شد',
        text: 'سفر سواری با موفقیت ثبت شد و پرداخت از طریق اعتبار آژانس پرداخت انجام گرفت همچنین پیام حاوی لینک بلیط به مسافر ارسال گردید',
        confirmButtonText: 'متوجه شدم',
        icon: 'success',
        customClass: {
            confirmButton: 'btn btn-primary waves-effect waves-light'
        },
        buttonsStyling: false
    });
</script>


<style>
    .validation-message {
        margin-top: .45rem;
        font-size: .8rem;
    }

    .success-payment {
        background-color: RGBA(var(--bs-success-rgb), .25) !important;
    }

    .not_enough_payment {
        background-color: RGBA(var(--bs-secondary-rgb), .25) !important;
    }

    .form-control {
        font-size: 1.07rem;
    }

    .form-check-label {
        padding-top: .42em !important;
        padding-bottom: .42em !important;
    }

    .border-divider, .border-gray-75 {
        --tw-border-opacity: 1;
        border-color: rgb(230 232 238 / var(--tw-border-opacity));
    }

    .input-forms {
        --bs-gutter-x: 0 !important;
        --bs-gutter-y: 0;
    }

    .border-1 {
        border-width: .063rem;
    }

    .rounded-5 {
        border-radius: 1.25rem;
    }

    .bg-white {
        --tw-bg-opacity: 1;
        background-color: rgb(255 255 255 / var(--tw-bg-opacity));
    }

    .shadow-7\.5__13__5__gray30 {

        --tw-shadow-colored: 0 1.875rem 3.125rem 1.25rem var(--tw-shadow-color);
    }

    .shadow-1__4\.5__black\/8, .shadow-7\.5__13__5__gray30 {
        box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);
    }

    .headline {
        justify-content: space-between;
    }

    *, :after, :before {
        box-sizing: border-box;
        border: 0 solid #a0a0b3;
    }

    .font-bold {
        font-weight: 700;
    }

    .text-blck {
        --tw-text-opacity: 1;
        color: rgb(41 38 61 / var(--tw-text-opacity));
    }

    .text-white {
        --tw-text-opacity: 1;
        color: rgb(255 255 255 / var(--tw-text-opacity));
    }

    .bg-divider {
        --tw-bg-opacity: 1;
        background-color: rgb(230 232 238 / var(--tw-bg-opacity));
        height: .063rem;
        flex: 1 1;
    }

    .steps {
        justify-content: space-between;
    }

    .step-container {
        justify-content: space-between;
        align-items: center;
        display: flex;
        flex: 1 1;
